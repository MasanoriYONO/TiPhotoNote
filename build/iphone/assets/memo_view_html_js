+" photo_id:"+photo_id+" a_memo_ct:"+a_memo_ct);}
Ti.API.debug("label_index:"+idx+" label_x:"+label_x+" label_y:"+label_y
+" photo_id:"+photo_id+" a_memo_ct:"+a_memo_ct);webView=Ti.UI.createWebView();webView.url="mv.html";win.add(webView);function jstrlen(str,len,i){len=0;str=escape(str);for(i=0;i<str.length;i++,len++){if(str.charAt(i)=="%"){if(str.charAt(++i)=="u"){i+=3;len++;}
i++;}}
return len;}
webView.addEventListener('load',function(e){});function zero_pad(v){var rel="00"+v;return rel.slice(-2);}
Ti.App.addEventListener("selectDropdownList",function(e){var label_text_color=webView.evalJS("document.getElementById('label_text_color').options[document.getElementById('label_text_color').options.selectedIndex].value");var label_back_color=webView.evalJS("document.getElementById('label_back_color').options[document.getElementById('label_back_color').options.selectedIndex].value");var label_text=webView.evalJS("document.getElementById('memo_text').value");Ti.API.debug("selectDropdownList click. text color:"+label_text_color
+' back:'+label_back_color
+' text:'+label_text);Ti.App.Properties.setString("label_text",label_text);Ti.App.Properties.setString("label_text_color",label_text_color);Ti.App.Properties.setString("label_back_color",label_back_color);win.close();});